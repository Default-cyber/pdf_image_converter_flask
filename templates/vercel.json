{
  "version": 2,
  "builds": [
    {
      "src": "app.py", 
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)", 
      "dest": "/static/$1"
    },
    {
      "src": "/outputs/(.*)", 
      "dest": "/outputs/$1"  
    },
    {
      "src": "/(.*)",
      "dest": "app.py" 
    }
  ]
}
